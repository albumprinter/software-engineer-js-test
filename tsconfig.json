{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "noEmitHelpers": false
    }
  },
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "alwaysStrict": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
